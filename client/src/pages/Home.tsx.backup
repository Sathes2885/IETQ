                  <p className="text-xl font-black mb-0">Grades 1-10</p>
                </div>
              </div>
              <div className="w-full lg:flex lg:justify-end">
                <div ref={heroImageRef} className="hero-image rounded-xl overflow-hidden shadow-xl bg-white/10 backdrop-blur-sm p-4 md:p-6 w-full parallax-content parallax-slow">
                  <div className="text-center mb-4">
                    <h3 className="text-xl md:text-2xl font-bold">Unlock Your Potential</h3>
                    <p className="mt-2 text-white/80">Showcase talents, develop skills, and rise as future-ready leaders!</p>
                  </div>
                  <div className="bg-white/30 p-4 rounded-lg backdrop-blur-sm shadow-lg competition-container relative z-20">
                    <h4 className="font-bold text-xl mb-4 text-center">Competition</h4>
                    <div className="relative">
                      {/* Vertical timeline/competition path */}
                      <div className="absolute left-14 sm:left-[120px] top-0 bottom-0 w-1 bg-accent/80 competition-line"></div>
                      
                      {/* Competition stages - mobile-friendly card style */}
                      <div className="relative z-30 flex flex-col space-y-8 transform scale-95">
                        {/* Quizz Whizz */}
                        <div className="flex competition-stage competition-stage-1">
                          <div className="flex-shrink-0">
                            <div className="flex items-center justify-center w-12 h-12 rounded-full bg-accent text-primary-900 text-xl font-bold border-4 border-white shadow-lg animate-pulse">
                              QW
                            </div>
                          </div>
                          <div className="ml-4 bg-white p-4 rounded-lg shadow-lg border-l-4 border-accent flex-1">
                            <h4 className="text-lg font-bold">Quizz Whizz</h4>
                            <p className="text-gray-600 mt-1">Knowledge-based assessment across multiple subjects</p>
                            <div className="mt-2 flex items-center text-accent font-medium text-sm">
                              <svg className="h-4 w-4 mr-1 animate-bounce" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                <path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="M13 10V3L4 14h7v7l9-11h-7z" />
                              </svg>
                              Start your journey here
                            </div>
                          </div>
                        </div>
                        
                        {/* Think Tank */}
                        <div className="flex competition-stage competition-stage-2">
                          <div className="flex-shrink-0">
                            <div className="flex items-center justify-center w-12 h-12 rounded-full bg-gradient-to-br from-blue-600 to-cyan-400 text-white text-xl font-bold border-4 border-white shadow-lg animate-pulse">
                              TT
                            </div>
