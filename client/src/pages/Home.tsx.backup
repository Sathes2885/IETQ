import { useEffect, useRef } from 'react';
import { Link } from 'wouter';
import { Button } from '@/components/ui/button';
import { Card, CardContent } from '@/components/ui/card';
import Navbar from '@/components/layout/Navbar';
import Footer from '@/components/layout/Footer';
import { gsap } from 'gsap';

export default function Home() {
  const gsapHeadingRef = useRef<HTMLHeadingElement>(null);
  const ietqFullFormRef = useRef<HTMLDivElement>(null);
  const azVirtualRef = useRef<HTMLDivElement>(null);
  const heroHeadingRef = useRef<HTMLHeadingElement>(null);
  const heroTextRef = useRef<HTMLParagraphElement>(null);
  const heroButtonsRef = useRef<HTMLDivElement>(null);
  const heroImageRef = useRef<HTMLDivElement>(null);
  const featureCardsRef = useRef<HTMLDivElement>(null);
  const rewardsContainerRef = useRef<HTMLDivElement>(null);

  useEffect(() => {
    // GSAP letter animation for "IETQ" and "EMPOWERING TOMORROW'S LEADERS"
    if (gsapHeadingRef.current) {
      // Get each word container
      const wordContainers = gsapHeadingRef.current.querySelectorAll('.heading-word');
      const wordContainersArray = Array.from(wordContainers);
      
      // Set initial state for each container
      gsap.set(wordContainersArray, {
        y: 60,
        opacity: 0
      });
      
      // Create a master timeline for the heading animation
      const masterTl = gsap.timeline();
      
      // First, special animation for IETQ title
      const ietqContainer = wordContainersArray[0];
      const ietqLetters = ietqContainer.querySelectorAll('span');
      const ietqLettersArray = Array.from(ietqLetters);
      
      // Set initial state for IETQ letters
      gsap.set(ietqLettersArray, {
        scale: 0,
        opacity: 0,
        rotationY: -180,
      });
      
      // IETQ Timeline
      const ietqTl = gsap.timeline();
      
      // Animate the IETQ container first
      ietqTl.to(ietqContainer, {
        y: 0,
        opacity: 1,
        duration: 0.6,
        ease: "back.out(1.7)"
      });
      
      // Then give IETQ letters a dramatic 3D flip-in animation
      ietqTl.to(ietqLettersArray, {
        scale: 1.2,
        opacity: 1,
        rotationY: 0,
        stagger: 0.15,
        duration: 0.7,
        ease: "back.out(2)",
        transformOrigin: "center center"
      });
      
      // Scale back to normal size with a slight bounce
      ietqTl.to(ietqLettersArray, {
        scale: 1,
        duration: 0.5,
        ease: "elastic.out(1.2, 0.5)"
      }, "-=0.2");
      
      // Add some shine effect to the gold letters
      ietqTl.to(ietqContainer, {
        backgroundPosition: "200%",
        duration: 1.5,
        ease: "sine.inOut"
      }, "-=0.3");
      
      // Animate the full form text if it exists
      if (ietqFullFormRef.current) {
        gsap.set(ietqFullFormRef.current, {
          opacity: 0,
          y: 10
        });
        
        ietqTl.to(ietqFullFormRef.current, {
          opacity: 1,
          y: 0,
          duration: 0.7,
          delay: 0.2,
          ease: "power2.out"
        });
      }
      
      // Add the IETQ animation to master timeline
      masterTl.add(ietqTl);
      
      // Then handle the rest of the words (starting from second word)
      for (let i = 1; i < wordContainersArray.length; i++) {
        const container = wordContainersArray[i];
        const letters = container.querySelectorAll('span');
        const lettersArray = Array.from(letters);
        
        // Set initial state for letters in this line
        gsap.set(lettersArray, {
          y: -40,
          opacity: 0,
          rotateX: -90
        });
        
        // Create a timeline for this word
        const wordTl = gsap.timeline();
        
        // First, animate in the container
        wordTl.to(container, {
          y: 0,
          opacity: 1,
          duration: 0.5,
          ease: "power2.out"
        });
        
        // Then animate each letter with elastic motion
        wordTl.to(lettersArray, {
          y: 0,
          opacity: 1,
          rotateX: 0,
          stagger: 0.08,
          duration: 0.8,
          ease: "elastic.out(1, 0.3)"
        }, "-=0.3");
        
        // Add this word's timeline to the master timeline with a delay
        masterTl.add(wordTl, "-=0.3");
      }
      
      // After all animations, add continuous effects
      masterTl.add(() => {
        // Add a pulsing glow to IETQ
        gsap.to(ietqContainer, {
          textShadow: "0 0 15px rgba(255,215,0,0.8), 0 0 20px rgba(255,215,0,0.4)",
          duration: 1.5,
          repeat: -1,
          yoyo: true,
          ease: "sine.inOut"
        });
        
        // Select all non-IETQ letter spans (for the rest of the heading)
        const nonIetqLetters = [];
        for (let i = 1; i < wordContainersArray.length; i++) {
          nonIetqLetters.push(...Array.from(wordContainersArray[i].querySelectorAll('span')));
        }
        
        // Animate with a gentle floating motion
        gsap.to(nonIetqLetters, {
          y: "-=5",
          duration: 1.5,
          stagger: {
            each: 0.05,
            from: "random",
            grid: "auto"
          },
          repeat: -1,
          yoyo: true,
          ease: "sine.inOut"
        });
      });
    }

    // GSAP animation for A-Z Virtual proudly presents
    if (azVirtualRef.current) {
      gsap.fromTo(azVirtualRef.current,
        { opacity: 0, scale: 0.8, y: 20 },
        { 
          opacity: 1, 
          scale: 1, 
          y: 0, 
          duration: 1,
          delay: 1.5, 
          ease: "back.out(1.7)",
          onComplete: () => {
            // Add a gentle bounce effect after appearing
            gsap.to(azVirtualRef.current, {
              y: -5,
              duration: 1.2,
              repeat: -1,
              yoyo: true,
              ease: "sine.inOut"
            });
          }
        }
      );
    }

    // Hero section animation
    if (heroHeadingRef.current) {
      gsap.from(heroHeadingRef.current, {
        opacity: 0,
        y: 30,
        duration: 0.8,
        ease: "power3.out"
      });
    }

    if (heroTextRef.current) {
      gsap.from(heroTextRef.current, {
        opacity: 0,
        y: 30,
        duration: 0.8,
        delay: 0.2,
        ease: "power3.out"
      });
    }

    if (heroButtonsRef.current) {
      gsap.from(heroButtonsRef.current, {
        opacity: 0,
        y: 30,
        duration: 0.8,
        delay: 0.4,
        ease: "power3.out"
      });
    }

    if (heroImageRef.current) {
      gsap.from(heroImageRef.current, {
        opacity: 0,
        x: 50,
        duration: 1,
        delay: 0.3,
        ease: "power3.out"
      });
    }

    // Continuous circular animation for competition stages
    const competitionObserver = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          // First animate the line (permanently visible)
          const line = document.querySelector('.competition-line');
          if (line) {
            gsap.fromTo(line, 
              { height: 0, opacity: 0 }, 
              { height: "100%", opacity: 1, duration: 1, ease: "power2.inOut" }
            );
          }
          
          // Get all three competition stages
          const stage1 = document.querySelector('.competition-stage-1');
          const stage2 = document.querySelector('.competition-stage-2');
          const stage3 = document.querySelector('.competition-stage-3');
          
          if (stage1 && stage2 && stage3) {
            // Show all stages initially
            gsap.set([stage1, stage2, stage3], { opacity: 1, x: 0, scale: 1 });
            
            // Create a continuous timeline that repeats
            const tl = gsap.timeline({ 
              repeat: -1, // Infinite repeat
              repeatDelay: 0.5
            });
            
            // Stage 1 focus animation
            tl.to(stage1, { 
              scale: 1.05, 
              filter: "brightness(1.1)",
              boxShadow: "0 10px 25px rgba(0,0,0,0.1)",
              duration: 0.7, 
              ease: "back.out(1.7)" 
            })
            .to(stage1, { 
              scale: 1, 
              filter: "brightness(1)",
              boxShadow: "none",
              duration: 0.7, 
              ease: "back.out(1.2)" 
            }, "+=1.5") // Hold for 1.5s
            
            // Stage 2 focus animation
            .to(stage2, { 
              scale: 1.05, 
              filter: "brightness(1.1)",
              boxShadow: "0 10px 25px rgba(0,0,0,0.1)",
              duration: 0.7, 
              ease: "back.out(1.7)" 
            }, "+=0.3")
            .to(stage2, { 
              scale: 1, 
              filter: "brightness(1)",
              boxShadow: "none",
              duration: 0.7, 
              ease: "back.out(1.2)" 
            }, "+=1.5") // Hold for 1.5s
            
            // Stage 3 focus animation
            .to(stage3, { 
              scale: 1.05, 
              filter: "brightness(1.1)",
              boxShadow: "0 10px 25px rgba(0,0,0,0.1)",
              duration: 0.7, 
              ease: "back.out(1.7)" 
            }, "+=0.3")
            .to(stage3, { 
              scale: 1, 
              filter: "brightness(1)",
              boxShadow: "none",
              duration: 0.7, 
              ease: "back.out(1.2)" 
            }, "+=1.5"); // Hold for 1.5s
          }
          
          // Only observe once
          competitionObserver.unobserve(entry.target);
        }
      });
    }, { threshold: 0.3 });
    
    // Start observing the competition container
    const competitionContainer = document.querySelector('.competition-container');
    if (competitionContainer) {
      competitionObserver.observe(competitionContainer);
    }
      
    // Features animation (cards)
    if (featureCardsRef.current) {
      const cards = featureCardsRef.current.querySelectorAll('.stat-card');
      gsap.from(cards, {
        opacity: 0,
        y: 30,
        duration: 0.6,
        stagger: 0.15,
        ease: "back.out(1.2)",
        delay: 0.6
      });
    }

    // Add rewards left-to-right animation
    const rewardsObserver = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          // Animate rewards cards from left to right
          const rewardCards = entry.target.querySelectorAll('.reward-card');
          rewardCards.forEach(card => {
            card.classList.add('animated');
          });
          
          // Only observe once
          rewardsObserver.unobserve(entry.target);
        }
      });
    }, { threshold: 0.2 });
    
    // Start observing the rewards container
    if (rewardsContainerRef.current) {
      rewardsObserver.observe(rewardsContainerRef.current);
    }

    // Add intersection observer for section animations
    const sections = document.querySelectorAll('section:not(:first-child)');
    const observer = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          // Animate the section
          gsap.from(entry.target.querySelector('h2'), {
            opacity: 0,
            y: 20,
            duration: 0.7,
            ease: "power2.out"
          });
          
          gsap.from(entry.target.querySelector('p'), {
            opacity: 0,
            y: 20,
            duration: 0.7,
            delay: 0.1,
            ease: "power2.out"
          });
        }
      });
    }, { threshold: 0.2 });

    sections.forEach(section => {
      observer.observe(section);
    });

    return () => {
      // Clean up all observers
      observer.disconnect();
      competitionObserver.disconnect();
      rewardsObserver.disconnect();
    };
  }, []);

  return (
    <div className="min-h-screen flex flex-col">
      <Navbar />
      
      {/* Hero Section with Parallax */}
      <section className="parallax-container relative text-white py-6 md:py-12 overflow-hidden">
        {/* Parallax background elements */}
        <div className="parallax-layer parallax-layer-back bg-gradient-to-br from-blue-900 via-purple-900 to-indigo-900"></div>
        
        {/* Animated background shapes */}
        <div className="parallax-shape shape-1" style={{ background: "radial-gradient(circle, rgba(255,215,0,0.15) 0%, rgba(255,215,0,0) 70%)" }}></div>
        <div className="parallax-shape shape-2" style={{ background: "radial-gradient(circle, rgba(255,255,255,0.1) 0%, rgba(255,255,255,0) 70%)" }}></div>
        
        <div className="content-wrapper relative z-10">
          <div className="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div className="flex flex-col lg:grid lg:grid-cols-2 lg:gap-8 items-center">
              <div className="mb-10 lg:mb-0 parallax-content parallax-fastest">
                {/* Animated headline with students lifting the title */}
                <div className="mb-6 overflow-hidden relative">
                  <div className="relative z-10">
                    <div className="bg-gradient-to-r from-purple-500 via-accent to-pink-500 text-transparent bg-clip-text mx-auto relative">
                      <h1 ref={gsapHeadingRef} className="text-3xl sm:text-4xl md:text-5xl lg:text-6xl font-black tracking-wider text-center font-education drop-shadow-lg flex flex-col items-center leading-tight">
                        <div className="overflow-hidden mb-8">
                          <div className="flex flex-col items-center">
                            <div className="heading-word text-5xl sm:text-6xl md:text-7xl lg:text-8xl font-black tracking-widest font-rubik" style={{ color: "#ffd700", textShadow: "0 0 15px rgba(255,215,0,0.7), 0 0 30px rgba(255,215,0,0.4)" }}>
                              <span className="inline-block">I</span>
                              <span className="inline-block">E</span>
                              <span className="inline-block">T</span>
                              <span className="inline-block">Q</span>
                            </div>
                            <div className="text-2xl sm:text-3xl md:text-4xl lg:text-5xl text-white/95 mt-3 font-medium tracking-wider font-rubik" ref={ietqFullFormRef} style={{ color: "#f5f5f5", textShadow: "0 0 12px rgba(255,255,255,0.6)" }}>
                              India's Emerging Talent Quest
                            </div>
                          </div>
                        </div>
                        <div className="overflow-hidden mt-2">
                          <div className="heading-word">
                            <span className="inline-block">E</span>
                            <span className="inline-block">M</span>
                            <span className="inline-block">P</span>
                            <span className="inline-block">O</span>
                            <span className="inline-block">W</span>
                            <span className="inline-block">E</span>
                            <span className="inline-block">R</span>
                            <span className="inline-block">I</span>
                            <span className="inline-block">N</span>
                            <span className="inline-block">G</span>
                          </div>
                        </div>
                        <div className="overflow-hidden">
                          <div className="heading-word mt-1">
                            <span className="inline-block">T</span>
                            <span className="inline-block">O</span>
                            <span className="inline-block">M</span>
                            <span className="inline-block">O</span>
                            <span className="inline-block">R</span>
                            <span className="inline-block">R</span>
                            <span className="inline-block">O</span>
                            <span className="inline-block">W</span>
                            <span className="inline-block">'</span>
                            <span className="inline-block">S</span>
                          </div>
                        </div>
                        <div className="overflow-hidden">
                          <div className="heading-word mt-1">
                            <span className="inline-block">L</span>
                            <span className="inline-block">E</span>
                            <span className="inline-block">A</span>
                            <span className="inline-block">D</span>
                            <span className="inline-block">E</span>
                            <span className="inline-block">R</span>
                            <span className="inline-block">S</span>
                          </div>
                        </div>
                      </h1>
                    </div>
                  </div>
                  
                  {/* A-Z Virtual proudly presents */}
                  <div className="mt-8 text-center">
                    <div ref={azVirtualRef} className="inline-block bg-gradient-to-r from-purple-800/80 to-blue-800/80 backdrop-blur-md py-3 px-8 rounded-full shadow-xl border border-white/20 opacity-0">
                      <div className="flex items-center justify-center space-x-3">
                        <span className="text-xl font-bold text-white font-education tracking-wide">A-Z Virtual</span>
                        <span className="text-base font-semibold text-accent italic font-serif">proudly presents</span>
                      </div>
                    </div>
                  </div>
                </div>
                
                <h1 ref={heroHeadingRef} className="hero-heading text-4xl font-bold sm:text-5xl lg:text-6xl font-education">
                  <span className="block">"Discover. Develop. Dazzle."</span>
                </h1>
                <h2 className="text-2xl md:text-3xl font-bold mt-2 text-accent">
                  Get Ready for the Ultimate Student Showdown – IETQ is Here!
                </h2>
                <p className="text-xl font-bold mt-4 text-white">
                  GO BEYOND YOUR CLASSROOM
                </p>
                <p ref={heroTextRef} className="hero-text mt-4 text-base md:text-lg">
                  Calling all curious thinkers, bold creators, and hidden superstars! IETQ (India's Emerging Talent Quest) is your all-India stage to compete, collaborate, and celebrate your unique talents.
                </p>
                <div className="bg-accent/90 backdrop-blur-sm p-4 rounded-lg border-l-4 border-white mt-4 mb-6 transform rotate-1 shadow-lg">
                  <p className="text-xl font-bold text-primary-950 text-center">
                    Are you ready to rise? Join the Quest. Be the Next Talent Icon!
                  </p>
                </div>
                <div ref={heroButtonsRef} className="hero-buttons mt-4 flex flex-wrap gap-4">
                  <Link href="/register">
                    <Button 
                      size="lg" 
                      className="bg-accent text-primary-950 hover:bg-accent/90 font-bold text-base px-6 py-6 h-auto shadow-lg animate-pulse"
                    >
                      Register Now 🚀
                    </Button>
                  </Link>
                  <Button 
                    size="lg" 
                    className="bg-blue-600 border-blue-500 text-white hover:bg-blue-700 font-medium text-base px-6 py-6 h-auto shadow-md"
                    onClick={() => {
                      const elementToScrollTo = document.getElementById('know-more');
                      if (elementToScrollTo) {
                        elementToScrollTo.scrollIntoView({ behavior: 'smooth' });
                      }
                    }}
                  >
                    Know More ↓
                  </Button>
