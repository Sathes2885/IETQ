                        {/* Me@My Best */}
                        <div className="flex competition-stage competition-stage-3">
                          <div className="flex-shrink-0">
                            <div className="flex items-center justify-center w-12 h-12 rounded-full bg-gradient-to-br from-purple-600 to-pink-500 text-white text-xl font-bold border-4 border-white shadow-lg animate-pulse">
                              MB
                            </div>
                          </div>
                          <div className="ml-4 bg-white p-4 rounded-lg shadow-lg border-l-4 border-purple-500 flex-1">
                            <h4 className="text-lg font-bold text-purple-700">Me@My Best</h4>
                            <p className="text-gray-600 mt-1">Talent showcase and final presentation stage</p>
                            <div className="mt-2 flex items-center text-purple-600 font-medium text-sm">
                              <svg className="h-4 w-4 mr-1 animate-ping" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                <path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="M11.049 2.927c.3-.921 1.603-.921 1.902 0l1.519 4.674a1 1 0 00.95.69h4.915c.969 0 1.371 1.24.588 1.81l-3.976 2.888a1 1 0 00-.363 1.118l1.518 4.674c.3.922-.755 1.688-1.538 1.118l-3.976-2.888a1 1 0 00-1.176 0l-3.976 2.888c-.783.57-1.838-.197-1.538-1.118l1.518-4.674a1 1 0 00-.363-1.118l-3.976-2.888c-.784-.57-.38-1.81.588-1.81h4.914a1 1 0 00.951-.69l1.519-4.674z" />
